{
  "schema": "schema.graphql",
  "documents": "**/*.graphql",
  "extensions": {
    "strawberryShake": {
      "name": "RockShopClient",
      "namespace":"RockShop.GraphQL.Client.Console",
      "url": "http://localhost:5034/graphql",
      "dependencyInjection":true,
      "records":{
        "inputs":false,
        "entities":false
      },
      "transportProfiles":[
        {
          "default":"Http",
          "subscription":"WebSocket"
        }
      ]
    }
  }
}